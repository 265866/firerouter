{
  "interface": {
    "phy": {
      "eth0": {
        "enabled": true
      },
      "eth1": {
        "enabled": true
      },
      "eth2": {
        "enabled": true
      },
      "eth3": {
        "enabled": true,
        "dhcp": true
      }
    },
    "bridge": {
      "br0": {
        "ipv4": "10.0.0.1/24",
        "intf": [
          "eth0",
          "eth1",
          "eth2"
        ],
        "enabled": true
      }
    }
  },
  "routing": {
    "global": {
      "default": {
        "viaIntf": "eth3"
      }
    }
  },
  "dns": {
    "default": {
      "nameservers": [
        "114.114.114.114",
        "1.1.1.1"
      ]
    },
    "br0": {
      "useNameserversFromWAN": true
    }
  },
  "nat": {
    "br0_eth3": {
      "in": "br0",
      "out": "eth3"
    }
  },
  "dhcp": {
    "br0": {
      "gateway": "10.0.0.1",
      "subnetMask": "255.255.255.0",
      "nameservers": [
        "10.0.0.1"
      ],
      "searchDomain": [
        ".lan"
      ],
      "range": {
        "from": "10.0.0.10",
        "to": "10.0.0.250"
      },
      "lease": 86400
    }
  }
}
