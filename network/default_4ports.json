{
  "interface": {
    "phy": {
      "eth0": {
        "enabled": true
      },
      "eth1": {
        "enabled": true
      },
      "eth2": {
        "enabled": true,
        "ipv4": "192.168.1.111/24",
        "gateway": "192.168.1.2"
      },
      "eth3": {
        "enabled": true,
        "dhcp": true
      }
    },
    "bridge": {
      "br0": {
        "ipv4": "10.0.0.1/24",
        "intf": [
          "eth0"
        ],
        "enabled": true
      },
      "br1": {
        "ipv4": "10.0.1.1/24",
        "intf": [
          "eth1"
        ],
        "enabled": true
      }
    }
  },
  "routing": {
    "global": {
      "default": {
        "viaIntf": "eth3"
      }
    },
    "br1": {
      "default": {
        "viaIntf": "eth2"
      }
    }
  },
  "dns": {
    "default": {
      "nameservers": [
        "114.114.114.114",
        "1.1.1.1"
      ]
    },
    "br0": {
      "useNameserversFromWAN": true
    },
    "br1": {
      "nameservers": [
        "8.8.8.8",
        "114.114.114.114"
      ]
    }
  },
  "nat": {
    "br0_eth3": {
      "in": "br0",
      "out": "eth3"
    },
    "br1_eth2": {
      "in": "br1",
      "out": "eth2"
    }
  },
  "dhcp": {
    "br0": {
      "gateway": "10.0.0.1",
      "subnetMask": "255.255.255.0",
      "nameservers": [
        "10.0.0.1"
      ],
      "searchDomain": [
        ".lan"
      ],
      "range": {
        "from": "10.0.0.10",
        "to": "10.0.0.250"
      },
      "lease": 86400
    }
  }
}
